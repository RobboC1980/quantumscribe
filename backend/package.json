{
  "name": "quantumscribe-backend",
  "version": "0.1.0",
  "type": "module",
  "main": "dist/app.js",
"scripts": {
  "build": "tsc -p ./tsconfig.json --noEmit false --skipLibCheck",
  "dev": "tsx watch --env-file=.env.local src/app.ts",
  "lint": "eslint src --ext .ts",
  "start": "node -r dotenv/config -r ts-node/register ./check-env.ts && node dist/app.js",
  "test": "vitest --run",
  "vercel-build": "tsc -p ./tsconfig.json --noEmit false --skipLibCheck",
  "stripe:listen": "stripe listen --forward-to http://localhost:${PORT:-3000}/api/stripe/webhook"
},
  "dependencies": {
    "@supabase/supabase-js": "^2.35.0",
    "@types/node-fetch": "^2.6.12",
    "@vercel/node": "^3.0.24",
    "bcryptjs": "^3.0.2",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.5.0",
    "express": "^4.18.2",
    "express-rate-limit": "^7.5.0",
    "helmet": "^6.2.0",
    "ioredis": "^5.3.2",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "node-fetch": "^3.3.2",
    "openai": "^4.96.0",
    "pg": "^8.15.5",
    "pino-http": "^10.4.0",
    "sse-broadcast": "^0.5.2",
    "stripe": "^13.11.0"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/compression": "^1.7.5",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/jsonwebtoken": "^9.0.9",
    "@types/morgan": "^1.9.9",
    "@types/node": "^20.10.0",
    "@types/pg": "^8.11.14",
    "@types/stripe": "^8.0.416",
    "@types/supertest": "^6.0.3",
    "jest": "^29.7.0",
    "supertest": "^7.1.0",
    "ts-node": "^10.9.2",
    "tsx": "^4.19.3",
    "typescript": "^5.3.3",
    "vitest": "^3.1.2"
  }
}
